  "metadata": {
    "title": "TL1 Command Examples",
    "description": "Common TL1 command examples for 1603 SM/SMX systems",
    "version": "1.0",
    "platforms": ["1603 SM", "1603 SMX"]
  },
  "examples": {
    "basic_commands": {
      "login": {
        "command": "ACT-USER",
        "syntax": "ACT-USER:[TID]:[UID]:[CTAG]::PID;",
        "example": "ACT-USER:NE01:ADMIN:1::PASSWORD;",
        "description": "Login to the system",
        "response": "NE01 25-10-07 14:30:00 M 1 COMPLD"
      },
      "logout": {
        "command": "CANC-USER",
        "syntax": "CANC-USER:[TID]:[UID]:[CTAG]::;",
        "example": "CANC-USER:NE01:ADMIN:2::;",
        "description": "Logout from the system",
        "response": "NE01 25-10-07 14:31:00 M 2 COMPLD"
      },
      "retrieve_header": {
        "command": "RTRV-HDR",
        "syntax": "RTRV-HDR:[TID]::[CTAG]::;",
        "example": "RTRV-HDR:NE01::3::;",
        "description": "Retrieve system header information",
        "response": "NE01 25-10-07 14:32:00 M 3 COMPLD\n   \"NE01:1603 SM:R12.3.4:25-10-07:14:32:00\""
      }
    },
    "monitoring_commands": {
      "retrieve_alarms": {
        "command": "RTRV-ALM-ALL",
        "syntax": "RTRV-ALM-ALL:[TID]::[CTAG]::;",
        "example": "RTRV-ALM-ALL:NE01::4::;",
        "description": "Retrieve all active alarms",
        "response": "NE01 25-10-07 14:33:00 M 4 COMPLD\n   \"1-1-1:CR,SA,12-25,\"\"LOS\"\"\"\n   \"1-2-3:MN,NSA,10-01,\"\"AIS\"\"\""
      },
      "retrieve_pm": {
        "command": "RTRV-PM-OC12",
        "syntax": "RTRV-PM-OC12:[TID]:[AID]:[CTAG]::;",
        "example": "RTRV-PM-OC12:NE01:OC12-1-1:5::;",
        "description": "Retrieve performance monitoring data for OC12",
        "response": "NE01 25-10-07 14:34:00 M 5 COMPLD\n   \"OC12-1-1:::ES=0,SES=0,UAS=0,CV=5,FC=0\""
      }
    },
    "configuration_commands": {
      "provision_oc12": {
        "command": "ENT-OC12",
        "syntax": "ENT-OC12:[TID]:[AID]:[CTAG]::;",
        "example": "ENT-OC12:NE01:OC12-1-2:6::;",
        "description": "Provision an OC12 port",
        "response": "NE01 25-10-07 14:35:00 M 6 COMPLD"
      },
      "delete_oc12": {
        "command": "DLT-OC12",
        "syntax": "DLT-OC12:[TID]:[AID]:[CTAG]::;",
        "example": "DLT-OC12:NE01:OC12-1-2:7::;",
        "description": "Delete an OC12 port",
        "response": "NE01 25-10-07 14:36:00 M 7 COMPLD"
      }
    },
    "maintenance_commands": {
      "operate_loopback": {
        "command": "OPR-LPBK-T1",
        "syntax": "OPR-LPBK-T1:[TID]:[AID]:[CTAG]::;",
