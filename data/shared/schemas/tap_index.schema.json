{
  "$schema":"https://json-schema.org/draft/2020-12/schema",
  "title":"TAP Index (array)",
  "type":"array",
  "items":{
    "type":"object",
    "required":["tap_id","title","alarm_types","severity","service_impact","applicable_models"],
    "properties":{
      "tap_id":{"type":"string"},
      "title":{"type":"string"},
      "alarm_types":{"type":"array","items":{"type":"string"}},
      "severity":{"type":"string"},
      "service_impact":{"type":"string"},
      "applicable_models":{"type":"array","items":{"type":"string"}}
    },
    "additionalProperties":true
  }
}
