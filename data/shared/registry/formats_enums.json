{
  "version": "2025-10-05.P1",
  "formats": {
    "OC12": { "regex": "^OC12-\\d+(?:-\\d+)?$", "desc": "OC12 port e.g., OC12-1 or OC12-1-1" },
    "T1":   { "regex": "^T1-\\d+-\\d+$", "desc": "T1 DS1 port e.g., T1-1-1" },
    "VT":   { "regex": "^VT1-\\d+-\\d+(?:-\\d+)?$", "desc": "VT path e.g., VT1-1-1" },
    "DS3":  { "regex": "^DS3-\\d+-\\d+$", "desc": "DS3 port" },
    "NEID": { "regex": "^[A-Z0-9-]{2,}$", "desc": "Network Element ID" },
    "IP":   { "regex": "^(?:\\d{1,3}\\.){3}\\d{1,3}$", "desc": "IPv4" },
    "CTAG": { "regex": "^\\d{1,6}$", "desc": "Correlation tag" }
  },
  "enums": {
    "NTFCNCDE": ["CR","MJ","MN","NA"],
    "DIR": ["1W","2W"],
    "TIMSRC": ["EXT","INT","LINE","PRS"],
    "LB_DIR": ["FACILITY","TERMINAL"],
    "ADMSTATE": ["IS","OOS-AUMA","OOS-MA,MT"],
    "COMPLETION": ["COMPLD","MDCMPLD","RTRV","PRTL","DENY","IPR","RLN","NACK"]
  },
  "completion_codes": ["COMPLD","MDCMPLD","RTRV","PRTL","DENY","IPR"],
  "error_codes": [
    {"code":"DENY","desc":"Access denied"},
    {"code":"IPR","desc":"Invalid parameter"},
    {"code":"PRTL","desc":"Partial result"}
  ],
  "global_parse_hints": {
    "completion_line": "^(COMPLD|MDCMPLD|DENY|IPR|PRTL);?$",
    "row_delimiter": ";",
    "field_kv_regex": "([A-Z0-9]+)=([^,;]+)"
  }
}
