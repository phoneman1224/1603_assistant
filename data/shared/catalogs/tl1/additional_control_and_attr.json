    "command_name": "Retrieve NE Attributes",
    "category": "Administration",
    "subcategory": "Attributes",
    "applicable_models": [
      "SM",
      "SMX"
    ],
    "syntax": "RTRV-ATTR-NE:[tid]::[ctag]::[PARAMS];",
    "parameters": {
      "tid": {
        "required": false,
        "description": "Target ID"
      },
      "ctag": {
        "required": false,
        "auto_generate": true
      },
      "PARAMS": {
        "required": false,
        "description": "Optional filters"
      }
    },
    "response_format": {
      "header": "COMPLD + date/time",
      "body_fields": [
        "NEID",
        "SWVER",
        "TIMEZONE",
        "LOCALE",
        "SYNCSTATE"
      ]
    },
    "safety_level": "safe",
    "service_affecting": false
  },
