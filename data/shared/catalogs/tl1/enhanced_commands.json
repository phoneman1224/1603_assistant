                "command_code": "ED-T1",
                "description": "Edit T1 Configuration",
                "platforms": ["1603_SM", "16034_SMX"],
                "parameters": {
                    "required": [
                        {
                            "name": "AID",
                            "type": "string",
                            "description": "Access Identifier",
                            "validation": "^\\d+-\\d+-\\d+$",
                            "example": "1-1-1"
                        }
                    ],
                    "optional": [
                        {
                            "name": "LINECODING",
                            "type": "string",
                            "description": "Line Coding",
                            "validation": "^(AMI|B8ZS)$",
                            "example": "B8ZS"
                        },
                        {
                            "name": "FMT",
                            "type": "string",
                            "description": "Frame Format",
                            "validation": "^(ESF|SF|SLC96)$",
                            "example": "ESF"
                        },
                        {
                            "name": "YELLOW",
                            "type": "string",
                            "description": "Yellow Alarm",
                            "validation": "^(Y|N)$",
                            "example": "N"
                        },
                        {
                            "name": "SENDA53",
                            "type": "string",
                            "description": "Send ANSI T1.403 Signal",
                            "validation": "^(Y|N)$",
                            "example": "Y"
                        },
                        {
                            "name": "SENDB53",
                            "type": "string",
                            "description": "Send Bellcore TR-TSY-000053 Signal",
                            "validation": "^(Y|N)$",
                            "example": "N"
                        }
                    ]
                },
                "response_format": {
                    "success": "<TID> <DATE> <TIME> M <CTAG> COMPLD",
                    "error": "<TID> <DATE> <TIME> M <CTAG> DENY\n   ERCD=\"<code>\"\n   \"<text>\""
                },
                "examples": [
                    {
