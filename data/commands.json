    "ENT-CRS-STS1": {
      "id": "ENT-CRS-STS1",
      "displayName": "Enter STS-1 Cross Connect",
      "platforms": ["1603_SM", "16034_SMX"],
      "category": "Cross Connect",
      "verb": "ENT",
      "object": "CRS",
      "modifier": "STS1",
      "description": "Enter a bi-directional STS-1 cross-connection between two facilities",
      "syntax": "ENT-CRS-STS1:[tid]:[ctag]::aidsts1from,aidsts1to,[cct];",
      "requires": ["TID", "CTAG", "AIDSTS1FROM", "AIDSTS1TO"],
      "optional": ["CCT"],
      "paramSchema": {
        "TID": {
          "type": "string",
          "maxLength": 20,
          "description": "Target Network Element identifier",
          "example": "SITE01"
        },
        "CTAG": {
          "type": "integer",
          "min": 1,
          "max": 999999,
          "description": "Correlation tag for command tracking",
          "example": "104"
        },
        "AIDSTS1FROM": {
          "type": "string",
          "description": "Source STS-1 facility identifier",
          "example": "LG1-STS1-1",
          "pattern": "LG[1-2]-STS1-[1-48]"
        },
        "AIDSTS1TO": {
          "type": "string",
          "description": "Destination STS-1 facility identifier", 
          "example": "LG2-STS1-1",
          "pattern": "LG[1-2]-STS1-[1-48]"
        },
        "CCT": {
          "type": "enum",
          "values": ["2WAY", "1WAY"],
          "description": "Cross-connection type",
          "default": "2WAY"
        }
      },
      "safety_level": "caution",
      "service_affecting": true,
      "response_format": "Confirmation of cross-connection creation",
      "provisioning": true
    },
