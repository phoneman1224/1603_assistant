    "RTRV-HDR": {
      "id": "RTRV-HDR",
      "displayName": "Retrieve Header",
      "platforms": [
        "1603 SM",
        "1603 SMX"
      ],
      "category": "Information Retrieval",
      "verb": "RTRV",
      "object": "HDR",
      "modifier": null,
      "description": "Retrieve system header information including software version, NE type, and system status",
      "syntax": "RTRV-HDR:[tid]:[aid]:[ctag]::;",
      "requires": [
        "TID",
        "CTAG"
      ],
      "optional": [
        "AID"
      ],
      "response_format": "M [ctag] COMPLD\n[system header data]\n;",
      "safety_level": "safe",
      "service_affecting": false,
      "examples": [
        "RTRV-HDR:SITE01::123::;",
        "RTRV-HDR:::456::;"
      ],
      "paramSchema": {
        "TID": {
          "type": "string",
          "maxLength": 20,
          "description": "Target Network Element identifier",
          "example": "SITE01",
          "hint": "Network element system identifier"
        },
        "AID": {
          "type": "string",
          "maxLength": 32,
          "description": "Access identifier (optional for header retrieval)",
          "example": "",
          "hint": "Leave blank for system-wide header"
        },
        "CTAG": {
          "type": "string",
          "maxLength": 6,
          "description": "Correlation tag for command tracking",
          "example": "123",
          "hint": "Unique identifier for this command"
        }
      }
    },
    "RTRV-ALM-ALL": {
      "id": "RTRV-ALM-ALL",
      "displayName": "Retrieve All Alarms",
      "platforms": [
        "1603 SM",
        "1603 SMX"
      ],
      "category": "Alarm Management",
      "verb": "RTRV",
      "object": "ALM",
      "modifier": "ALL",
      "description": "Retrieve all active alarms from the system",
      "syntax": "RTRV-ALM-ALL:[tid]:[aid]:[ctag]::[ntfcncod],[condtype],[srveff],[ocrdat],[locn];",
      "requires": [
        "TID",
        "CTAG"
      ],
      "optional": [
        "AID",
        "NTFCNCOD",
        "CONDTYPE",
        "SRVEFF",
        "OCRDAT",
        "LOCN"
      ],
      "paramSchema": {
        "TID": {
          "type": "string",
          "maxLength": 20,
          "description": "Target Network Element identifier",
          "example": "SITE01"
        },
        "AID": {
          "type": "string",
          "description": "Access identifier (optional - retrieves all if blank)",
          "example": "ALL",
          "default": ""
        },
        "CTAG": {
          "type": "integer",
          "min": 1,
          "max": 999999,
          "description": "Correlation tag for command tracking",
          "example": "101"
        },
        "NTFCNCOD": {
          "type": "enum",
          "values": [
            "CR",
            "MJ",
            "MN",
            "WR",
            "CL",
            "ALL"
          ],
          "description": "Notification code filter",
          "default": "ALL"
        },
        "CONDTYPE": {
          "type": "string",
          "description": "Condition type filter",
          "example": "LOS",
          "default": ""
        },
        "SRVEFF": {
          "type": "enum",
          "values": [
            "SA",
            "NSA",
            "ALL"
          ],
          "description": "Service affecting filter",
          "default": "ALL"
        },
        "OCRDAT": {
          "type": "string",
          "pattern": "\\d{2}-\\d{2}-\\d{2}",
          "description": "Occurrence date (YY-MM-DD)",
          "example": "25-10-07",
          "default": ""
        },
        "LOCN": {
          "type": "enum",
          "values": [
            "NEND",
            "FEND",
            "ALL"
          ],
          "description": "Location filter",
          "default": "ALL"
        }
      },
      "safety_level": "safe",
      "service_affecting": false,
      "response_format": "List of active alarms with details"
    },
    "OPR-LPBK-T1": {
      "id": "OPR-LPBK-T1",
      "displayName": "Operate T1 Loopback",
      "platforms": [
        "1603 SM",
        "1603 SMX"
      ],
      "category": "Testing & Diagnostics",
      "verb": "OPR",
      "object": "LPBK",
      "modifier": "T1",
      "description": "Operate loopback test on DS1 facility for testing and diagnostics",
      "syntax": "OPR-LPBK-T1:[tid]:aidt1:[ctag]::[locn],[orgn],[lpbktype];",
      "requires": [
        "TID",
        "AIDT1",
        "CTAG"
      ],
      "optional": [
        "LOCN",
        "ORGN",
        "LPBKTYPE"
      ],
      "paramSchema": {
        "TID": {
          "type": "string",
          "maxLength": 20,
          "description": "Target Network Element identifier",
          "example": "SITE01"
        },
        "AIDT1": {
          "type": "string",
          "description": "DS1 facility access identifier",
          "example": "DG1-T1-1",
          "pattern": "DG[1-3]-T1-[1-4]"
        },
        "CTAG": {
          "type": "integer",
          "min": 1,
          "max": 999999,
          "description": "Correlation tag for command tracking",
          "example": "102"
        },
        "LOCN": {
          "type": "enum",
          "values": [
            "NEND",
            "FEND"
          ],
          "description": "Location where loopback occurs",
          "default": "NEND"
        },
        "ORGN": {
          "type": "enum",
          "values": [
            "NEND",
            "FEND"
          ],
          "description": "Origin of signal to be looped back",
          "default": "NEND"
        },
        "LPBKTYPE": {
          "type": "enum",
          "values": [
            "FACILITY",
            "TERMINAL"
          ],
          "description": "Point at which signal is looped back",
          "default": "TERMINAL"
        }
      },
      "safety_level": "caution",
      "service_affecting": true,
      "response_format": "Confirmation of loopback operation"
    },
