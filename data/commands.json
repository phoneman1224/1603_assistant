      "safety_level": "caution",
      "service_affecting": true,
      "response_format": "Confirmation of cross-connection deletion",
      "provisioning": true
    },
    "RTRV-PM-STS1": {
      "id": "RTRV-PM-STS1",
      "displayName": "Retrieve STS-1 Performance Monitoring",
      "platforms": ["1603_SM", "16034_SMX"],
      "category": "Performance Monitoring",
      "verb": "RTRV",
      "object": "PM",
      "modifier": "STS1",
      "description": "Retrieve performance monitoring data for STS-1 facilities",
      "syntax": "RTRV-PM-STS1:[tid]:aidsts1:[ctag]::[montype],[monlev],[locn],[dirn],[tmper];",
      "requires": ["TID", "AIDSTS1", "CTAG"],
      "optional": ["MONTYPE", "MONLEV", "LOCN", "DIRN", "TMPER"],
      "paramSchema": {
        "TID": {
          "type": "string",
          "maxLength": 20,
          "description": "Target Network Element identifier",
          "example": "SITE01"
        },
        "AIDSTS1": {
          "type": "string",
          "description": "STS-1 facility identifier",
          "example": "LG1-STS1-1",
          "pattern": "LG[1-2]-STS1-[1-48]"
        },
        "CTAG": {
          "type": "integer",
          "min": 1,
          "max": 999999,
          "description": "Correlation tag for command tracking",
          "example": "106"
        },
        "MONTYPE": {
          "type": "enum",
          "values": ["ALL", "CVL", "ESL", "SESL", "UASL"],
          "description": "Monitored parameter type",
          "default": "ALL"
        },
        "MONLEV": {
          "type": "enum",
          "values": ["ALL", "PATH", "LINE", "SECTION"],
          "description": "Monitoring level",
          "default": "ALL"
        },
        "LOCN": {
          "type": "enum",
          "values": ["NEND", "FEND", "ALL"],
          "description": "Location",
          "default": "ALL"
        },
        "DIRN": {
          "type": "enum",
          "values": ["RCV", "TRMT", "ALL"],
          "description": "Direction",
          "default": "ALL"
        },
        "TMPER": {
          "type": "enum",
          "values": ["15-MIN", "1-DAY"],
          "description": "Time period",
          "default": "15-MIN"
        }
      },
      "safety_level": "safe",
      "service_affecting": false,
      "response_format": "Performance monitoring statistics"
    },
